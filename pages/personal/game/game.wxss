}
.dou{
  width:30rpx;
  height:30rpx;
  background: url(http://www.vij365.com/fomo3d/zydou.png) no-repeat;
  background-size:100%;
  float:left;
  margin:0 10rpx;
  transform: translateY(5px);
}
.font{
  font-size:26rpx;
  float: left;
  padding-left:12rpx;
}
.cloud_one_area{
   width: 390rpx;
   height:200rpx;
   background: url(http://www.vij365.com/fomo3d/cloud1.png) no-repeat;
   background-size: 100%;
   position: absolute;
   right:-160rpx;
   top:120rpx;
   z-index:777;
   animation: cloud_one_area 100s linear infinite;
}
@keyframes cloud_one_area {
  from {
    transform: translate3d(0, 0, 0);
  }
 
  to {
    transform: translate3d(-640rpx, 0, 0);
  }
}
.cloud_two_area{
  width: 220rpx;
  height:160rpx;
  background:url(http://www.vij365.com/fomo3d/cloud.png) no-repeat;
  background-size: 100% 100%;
  position: absolute;
  top:180rpx;
  left:-30rpx;
  z-index:777;
  animation: cloud 100s linear infinite; 
}
@keyframes cloud {
  from {
    transform: translate3d(0, 0, 0);
  }
 
  to {
    transform: translate3d(640rpx, 0, 0);
  }
}
.sun_area{
  width: 151rpx;
  height: 151rpx;
  border-radius: 50%;
  background: url(http://www.vij365.com/fomo3d/sun.png) no-repeat;
  background-size: 100%;
  position: absolute;
  left:120rpx;
  top:130rpx;
  z-index:776;
  animation: sun 12s linear infinite; 
}
@keyframes sun {
  0%{
    transform: scale(1.0);
  }
  33.3%{
    transform: scale(1.1);
  }
  66.6%{
    transform: scale(1.2);
  }
  100%{
    transform: scale(1.0);
  }
 
}
