  text-align: center;
  border-radius: 20rpx;
  animation: say 2.5s linear infinite; 
}
@keyframes say {
   10% {
    transform: rotate(15deg);
  }
  20% {
    transform: rotate(-10deg);
  }
  30% {
    transform: rotate(5deg);
  }
  40% {
    transform: rotate(-5deg);
  }
  50%,100% {
    transform: rotate(0deg);
  }
}
@keyframes monket {
  0%{
    left:-120rpx;
  }
  20%{
    left:-60rpx;
  }
  40%{
    left:0;
  }
  60%{
    left:60rpx;
  }
  80%{
    left:120rpx;
  }
  100%{
    left:180rpx;
  }
}
