}
.sssbtn button,.sssbtn button::after,.sssbtn button::before{
  border: none;
  border-radius: 0;
  color:#41A18D;
  font-size:32rpx;
  font-weight: normal;
  padding:10rpx 0;
}
.share-show-area::after{
