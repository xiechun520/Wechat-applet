.zhezhao {
    position: fixed;
    width: 100%;
    height: 100%;
    top: 0px;
    background: rgba(0, 0, 0, 0.4);
    overflow: hidden;
    z-index:999999;
}
.sq-area {
    width: 80%;
    height: 380rpx;
    background: #fff;
    margin: 0 auto;
    transform: translateY(90%);
    border-top-left-radius: 10rpx;
    border-top-right-radius: 10rpx;
}

.sq-name {
    font-size: 36rpx;
    font-weight: bold;
    text-align: center;
    padding: 40rpx 0 20rpx 0;
}

.sq-one-font {
    font-size: 32rpx;
    color: #787878;
    text-align: center;
    padding-bottom: 20rpx;
}

.sq-btn {
    position: absolute;
    bottom: 0;
    width: 100%;
    height: 120rpx;
    line-height: 120rpx;
}
.sq-btn button {
    font-size: 32rpx;
    border: none;
    border-radius: 0;
    position: absolute;
    bottom: 0;
    width: 100%;
    height: 100rpx;
    line-height: 100rpx;
    color: #41A18D;
    font-weight: bold;
    letter-spacing: 4rpx;
}

.sq-btn button::after {
    border: none;
    border-radius: 0;
}

.btn-center {
    padding: 10rpx 38rpx 0 38rpx;
    height: 100rpx;
    position: relative;
}
.welcome{
     width:70%;
     height:400rpx;
     background:#fff;
     transform:translateY(90%); 
     margin:0 auto;
     border-radius: 10rpx;
  }
.cancel{
  position: absolute;
